form.form-validate.mb-lg(role='form', name="theForm", novalidate='', ng-submit="vm.doSubmit()")
    .panel
        tabset(justified="true")
            tab(active="vm.tab1.active")
                tab-heading
                    | {{ vm.viewTranslatePath('TAB1') | translate}}
                    span.text-danger(title="{{'validator.FIELD-INVALID'|translate}}" , ng-show="utils.vtab('contentTab1')")
                        i.glyphicon.glyphicon-minus-sign
                div#contentTab1
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-ELDERLY-NAME') | translate}}:
                        .col-md-10(ng-if="vm._action_=='add' && vm.isAddNew")
                            .backfiller(backfiller,ng-model="vm.selectedElderly", input-name="elderly", fetch-rows="vm.queryElderlyPromise", required='',fetch-columns="vm.fetchElderlyColumnsPromise", picker-title="挑选老人" on-select="vm.selectElerlyForBackFiller(row)", on-search="vm.searchForBackFiller(keyword)")
                            span.text-danger(ng-show="utils.vinput(this.theForm,'elderly','autocomplete-required')") {{ "validator.REQUIRED" | translate}}
                        .col-md-10(ng-if="vm._action_=='read' || !vm.isAddNew")
                            .form-readable {{vm.model.elderly_name}}
                        //- label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-ELDERLY-NAME') | translate}}:
                        //- .col-md-10
                        //-     div#elderly(angucomplete-alt,input-name="elderly",minlength="1",field-required="true"t-searching='{{"label.TEXT-SEARCHING"|translate}}',text-no-results='{{"label.TEXT-NO-RESULTS"|translate}}',placeholder="",pause="400",selected-object="vm.selectElerly",remote-api-handler="vm.queryElderly",title-field="name",initial-value="vm.selectedElderly",input-class="form-control form-control-small")
                        //-     span.text-danger(ng-show="utils.vinput(this.theForm,'elderly','autocomplete-required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-DISEASE_EVALUATION') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='disease_evaluation',s-tree,s-tree-data="vm.disease_evaluation",s-tree-height="200", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{mode:'check',layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.base_on",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'disease_evaluation','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-SHIT') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='shit',s-tree,s-tree-data="vm.shit",s-tree-height="100", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.adl_shit",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'shit','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-groupzz
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-PEE') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='pee',s-tree,s-tree-data="vm.pee",s-tree-height="100", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.adl_pee",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'pee','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-DECORATOR') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='decorator',s-tree,s-tree-data="vm.decorator",s-tree-height="100", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.adl_decorator",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'decorator','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-WC') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='wc',s-tree,s-tree-data="vm.wc",s-tree-height="100", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.adl_wc",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'wc','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
