script(type='text/ng-template', id="dlg-backfiller-demo-pick.html")
    .ngdialog-title
        i.glyphicon.glyphicon-info-sign
        | {{ vm.translatePath('DLG-FETCH-DATA-GRID-TITLE') | translate }}
    .ngdialog-body.backfiller
        .search-form
            .input-group.page-search
                label.input-group-addon {{'grid.LABEL-SEARACH'|translate}}
                input.form-control(type="text",ng-model="vm.filter.$")
        .search-content
            .panel.panel-default.panel-grid
                table.grid.table.table-bordered.table-hover.table-striped
                    tbody
                        tr(ng-repeat="row in vm.rows|filter:vm.filter|paging:vm", ng-dblclick="vm.pickOne(row)")
                            td(width="80") {{::row.order }}
                            td(width="500") {{::row.name}}
                            td(width="80") {{::row.value}}
                .panel-footer
                    .row
                        .col-md-12
                            pagination.pagination-sm.pull-right(total-items="vm.page.totals",ng-model="vm.page.no",max-size="5",items-per-page="vm.page.size",boundary-links="true")
script(type='text/ng-template', id="dlg-backfiller-test-pick.html")
    .ngdialog-title
        i.glyphicon.glyphicon-info-sign
        | {{ vm.title }}
    .ngdialog-body.backfiller
        .search-form
            .input-group.page-search
                label.input-group-addon {{'grid.LABEL-SEARACH'|translate}}
                input.form-control(type="text",ng-model="vm.filter.$")
        .search-content
            .panel.panel-default.panel-grid
                table.grid.table.table-bordered.table-hover.table-striped
                    thead
                        tr
                            th.text-center(ng-repeat="column in vm.columns",width="{{column|width}}") {{column.label}}
                    tbody
                        tr(ng-repeat="row in vm.rows|filter:vm.filter|paging:vm", ng-dblclick="vm.pickOne(row)")
                            td(ng-repeat="column in vm.columns", width="{{column|width}}") {{::row[column.name] }}
                .panel-footer
                    .row
                        .col-md-12
                            pagination.pagination-sm.pull-right(total-items="vm.page.totals",ng-model="vm.page.no",max-size="5",items-per-page="vm.page.size",boundary-links="true")
