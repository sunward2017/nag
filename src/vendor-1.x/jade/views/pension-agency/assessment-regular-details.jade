                            span.text-danger(ng-show="utils.vinput(this.theForm,'eat','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-TRANSFER') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='transfer',s-tree,s-tree-data="vm.transfer",s-tree-height="100", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.adl_transfer",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'transfer','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-ACTIVITY') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='activity',s-tree,s-tree-data="vm.activity",s-tree-height="100", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.adl_activity",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'activity','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-DRESS') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='dress',s-tree,s-tree-data="vm.dress",s-tree-height="100", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.adl_dress",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'dress','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-STAIRS') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='stairs',s-tree,s-tree-data="vm.stairs",s-tree-height="100", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.adl_stairs",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'stairs','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-BATH') | translate}}:
                        .col-md-10
                            .tree.tree-dropdown(name='bath',s-tree,s-tree-data="vm.bath",s-tree-height="100", s-tree-template-url='tree-directive-default-renderer.html',s-tree-option="{layout:'dropdown',readonly:{{vm.readonly}} }",ng-model="vm.adl_bath",required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'bath','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        .col-md-2
                        .col-md-10
                            button.btn.btn-danger(ng-disabled="vm._action_=='read'",type="button",ng-click="vm.beginAssessment()") {{'button.BEGIN-ASSESSMENT'|translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-ASSESSMENT-GRADE') | translate}}:
                        .col-md-10
                            input.form-control(ng-disabled="true", type='text', name='assessment_grade',ng-model='vm.model.current_nursing_assessment_grade_name',placeholder='' ,required='')
                            span.text-danger(ng-show="utils.vinput(this.theForm,'assessment_grade','required')") {{ "validator.REQUIRED" | translate}}
                    fieldset: .form-group
                        label.col-md-2.control-label {{ vm.viewTranslatePath('TAB1-LABEL-NURSING-LEVEL') | translate}}:
                        .col-md-10(ng-if="!vm.readonly")
                            .btn-group
                                .btn.btn-primary(name='nursing_level',ng-disabled="!vm.model.current_nursing_assessment_grade",ng-repeat='nursing_level in vm.selectBinding.nursing_levels',ng-model='vm.model.nursingLevelId',ng-click='vm.setNursingLevel(nursing_level._id,nursing_level.name)',btn-radio="nursing_level._id",required='') {{nursing_level.name}}
                            span.text-danger(ng-show="utils.vinput(this.theForm,'nursing_level','required')") {{ "validator.REQUIRED" | translate}}
                         .col-md-10(ng-if="vm.readonly")
                            .btn-group
                                .btn.btn-primary.cursor-not-allowed(name='nursing_level',ng-repeat='nursing_level in vm.selectBinding.nursing_levels',ng-class="{'active':vm.model.nursingLevelId==nursing_level._id}") {{nursing_level.name}}



    .text-right.mt-lg
        button.btn.btn-warning(ng-if="!vm.readonly",type="button",ng-click="vm.cancel()") {{'button.CANCEL'|translate}}
        button.btn.btn-success(ng-if="vm._action_!=='read'",type="submit") {{'button.SAVE'|translate}}
        button.btn.btn-primary(ng-if="vm.readonly",type="button",ng-click="vm.cancel()") {{'button.RETURN'|translate}}